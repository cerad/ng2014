{# @CeradTourn/PersonPlan/Update/PersonPlanUpdateForm.html.twig   #}
<form
    id     = "cerad_tourn_person_plan"
    class  = "cerad_tourn_person_plan cerad_common_form1 app_form"
    action = "{{ path('cerad_tourn_person_plan_update',{ 'id' : person.id }) }}"
    method = "post" {{ form_enctype(form) }}
    novalidate
>
    <div>
    <fieldset><legend><span>Your Plan Information</span></legend>
        {{ form_rest(form.basic) }}
    </fieldset>
    </div>
    <div>
    <fieldset>
        <div class="controls">
            <input type="submit" name="personPlanUpdate" value="Update Plan" />
        </div>

     </fieldset>
    </div>
{{ form_rest(form) }}
</form>
